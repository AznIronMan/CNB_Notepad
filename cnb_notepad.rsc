{
  "launcher_sh": "#!/bin/bash\n\n# Set the path to your virtual environment\nVENV_PATH=\".venv-{HOSTNAME}\"\n\n# Activate the virtual environment\nsource \"$VENV_PATH/bin/activate\"\n\n# Navigate to the directory containing app.py (adjust if necessary)\ncd \"$(dirname \"$0\")\"\n\n# Run the app.py script\npython app.py\n\n# Deactivate the virtual environment when the app closes\ndeactivate",
  
  "launcher_bat": "@echo off\n:: Set the path to your virtual environment\nset VENV_PATH=.venv-{HOSTNAME}\n\n:: Activate the virtual environment\ncall \"%VENV_PATH%\\Scripts\\activate.bat\"\n\n:: Navigate to the directory containing app.py (adjust if necessary)\ncd /d \"%~dp0\"\n\n:: Run the app.py script\npython app.py\n\n:: Deactivate the virtual environment when the app closes\ncall deactivate",
  
  "launcher_vbs": "Set objShell = CreateObject(\"WScript.Shell\")\nstrPath = objShell.CurrentDirectory\n\n' Set the path to your virtual environment\nstrVenvPath = \".venv-{HOSTNAME}\"\n\n' Construct the command to run\nstrCommand = \"cmd /c \" & Chr(34) & _\n             strVenvPath & \"\\Scripts\\activate.bat && \" & _\n             \"cd /d \" & Chr(34) & strPath & Chr(34) & \" && \" & _\n             \"python app.py && \" & _\n             \"deactivate\" & Chr(34)\n\n' Run the command\nobjShell.Run strCommand, 0, True",

  "windows_shortcut": "Set oWS = WScript.CreateObject(\"WScript.Shell\")\nstrDesktop = oWS.SpecialFolders(\"Desktop\")\nSet oLink = oWS.CreateShortcut(strDesktop & \"\\CNB Notepad.lnk\")\noLink.TargetPath = \"wscript.exe\"\noLink.Arguments = \"\"\"\" & WScript.ScriptFullName & \"\"\"\"\noLink.WorkingDirectory = WScript.ScriptFullName.Replace(WScript.ScriptName, \"\")\noLink.IconLocation = WScript.ScriptFullName.Replace(WScript.ScriptName, \"\") & \"icon.ico\"\noLink.Save",

